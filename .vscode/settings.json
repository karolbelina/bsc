        "*.toc",
        "*.bcf",
        "*.run.xml"
    ],
    "[latex]": {
        "editor.rulers": [80]
    },
    
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-output-directory=.",
                "-jobname=W08_242499_2020_praca_in≈ºynierska",
                "-pdf",
                "-bibtex",
                "-e",
                "'$bibtex_fudge=0;'",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.recipe.default": "latexmk",
