{
    "latex-workshop.latex.outDir": "%DIR%",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.latex.clean.fileTypes": [
        "*.acn",
        "*.acr",
        "*.alg",
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.fdb_latexmk",
        "*.fls",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.log",
        "*.lol",
        "*.lot",
        "*.nav",
        "*.out",
        "*.snm",
        "*.synctex(busy)",
        "*.synctex.gz",
        "*.synctex.gz(busy)",
        "*.toc",
        "*.bcf",
        "*.run.xml"
    ],
    "[latex]": {
        "editor.rulers": [80]
    },
    
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-output-directory=.",
                "-jobname=W08_242499_2020_praca_in≈ºynierska",
                "-pdf",
                "-bibtex",
                "-e",
                "'$bibtex_fudge=0;'",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.recipe.default": "latexmk",
    "cSpell.language": "en,pl",
    "cSpell.ignorePaths": [
        "*",
        "!eng.tex",
        "!bibliography.bib"
    ],
    "cSpell.words": [
        "ANTLR",
        "EBNF",
        "Naur",
        "Rollup",
        "combinating",
        "combinators",
        "pushdown"
    ]
}
